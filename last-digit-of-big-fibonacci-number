#the task is find the last digit of big Fibonacci number (1 <= n <= 10 ** 7)

def fib_digit(n):
    a = [0, 1]
    for i in range(2, n + 1):
        a.append((a[i - 1] + a[i - 2]) % 10)
    return a[n]


def main():
    n = int(input())
    print(fib_digit(n))


if __name__ == "__main__":
    main()
